{
    "name": "ralphjsmit/tall-interactive",
    "description": "Create forms, modals and slide-overs with ease.",
    "keywords": [
        "ralphjsmit",
        "laravel",
        "tall-interactive"
    ],
    "homepage": "https://github.com/ralphjsmit/tall-interactive",
    "license": "MIT",
    "authors": [
        {
            "name": "Ralph J. Smit",
            "email": "rjs@ralphjsmit.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "filament/forms": "^2.16",
        "illuminate/contracts": "^8.73|^9.0|^10.0",
        "livewire/livewire": "^2.8",
        "ryangjchandler/blade-tabler-icons": "^1.1|^2.0",
        "spatie/laravel-package-tools": "^1.9.2",
        "usernotnull/tall-toasts": "^1.5"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.10|^6.0",
        "nunomaduro/larastan": "^1.0",
        "orchestra/testbench": "^6.22|^7.0|^8.0",
        "pestphp/pest": "^1.22",
        "pestphp/pest-plugin-laravel": "^1.4",
        "phpstan/extension-installer": "^1.1",
        "phpunit/phpunit": "^9.6",
        "spatie/invade": "^1.0",
        "spatie/laravel-ray": "^1.27"
    },
    "autoload": {
        "psr-4": {
            "RalphJSmit\\Tall\\Interactive\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RalphJSmit\\Tall\\Interactive\\Tests\\": "tests"
        }
    },
    "scripts": {},
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "RalphJSmit\\Tall\\Interactive\\ActionablesServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": []
}